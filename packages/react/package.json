{
  "name": "@query-api/react",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "Craft CMS Query Builder for React",
  "repository": "https://github.com/samuelreichor/query-api/tree/main/packages/react",
  "author": "Samuel Reichoer",
  "bugs": {
    "url": "https://github.com/samuelreichor/query-api/issues"
  },
  "homepage": "https://samuelreichor.at/libraries/query-api-react",
  "keywords": [
    "Craft CMS",
    "Query Builder",
    "Query API",
    "React"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "default": "./dist/core.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "default": "./dist/react.js"
    }
  },
  "dependencies": {
    "@query-api/js": "*"
  }
}
