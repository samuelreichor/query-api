{
  "compilerOptions": {
    "isolatedModules": true,
    "lib": ["ESNext", "dom", "dom.iterable"],
    "module": "Preserve",
    "moduleResolution": "bundler",
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "noEmit": true,
  },
  "exclude": ["node_modules", "dist", "build"]
}
